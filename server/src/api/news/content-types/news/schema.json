{
  "kind": "collectionType",
  "collectionName": "newsfeed",
  "info": {
    "singularName": "news",
    "pluralName": "newsfeed",
    "displayName": "Noticias",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "titulo": {
      "type": "string",
      "required": true
    },
    "url": {
      "type": "string",
      "unique": true,
      "required": true,
      "regex": "^[A-Za-z0-9-]+$"
    },
    "fecha": {
      "type": "datetime",
      "required": true,
      "unique": false
    },
    "imagen": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "subtitulo": {
      "type": "text",
      "required": true
    },
    "cuerpo": {
      "type": "dynamiczone",
      "components": [
        "posts.imagen",
        "posts.texto",
        "posts.imagen-con-texto",
        "posts.encabezado",
        "posts.tabla"
      ]
    }
  }
}
